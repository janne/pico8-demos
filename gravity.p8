version 27
__lua__
-- gRAVITY fORCE

function _init()
  p = {x=64,y=64,dx=0,dy=0,a=0}
  shots = {}
end

function draw_debug()
	print("x"..flr(p.x)..",y"..flr(p.y)..",a"..flr(p.a*360),26,10)
end

function rot(x,y,a)
 local c=cos(-a)
 local s=sin(-a)
 return {x=x*c-y*s,y=y*c+x*s}
end

function ship(a)
 local width=3
 local height=4
 return {
  rot(0,-height,a),
  rot(width,height,a),
	 rot(0,0.7*height,a),
	 rot(-width,height,a)
 }
end

function draw_ship(x,y,a)
 local p=ship(a)
	line(x+p[1].x,y+p[1].y,x+p[2].x,y+p[2].y)
	line(x+p[3].x,y+p[3].y)
	line(x+p[4].x,y+p[4].y)
	line(x+p[1].x,y+p[1].y)
end

function is_solid(x,y)
 return fget(mget(flr(x/8),flr(y/8)),0)
end

function col(x,y,a)
 local p=ship(a)
 return is_solid(x+p[1].x,y+p[1].y) or
	 is_solid(x+p[2].x,y+p[2].y) or
	 is_solid(x+p[4].x,y+p[4].y)
end

function draw_dashboard()
 camera()
 rectfill(0,120,127,127,5)
	print("fUEL "..0,1,122,6)
end

function _draw()
 cls(1)
 map()
	draw_ship(p.x,p.y,p.a)
	for shot in all(shots) do
	 pset(shot.x, shot.y)	 
	end
	draw_dashboard()
	-- draw_debug()
end

function crash()
 _init()
end

function _update()
 -- turning
 if(btn(‚¨ÖÔ∏è) and p.dy != 0) p.a=(p.a-10/360)%1
 if(btn(‚û°Ô∏è) and p.dy != 0) p.a=(p.a+10/360)%1

	-- camera
	local cam_x_max=max(0,p.x-64)
	local cam_x=min(23*8-120,cam_x_max)
	local cam_y_max=max(0,p.y-64)
	local cam_y=min(31*8-120,cam_y_max)
	camera(cam_x,cam_y)

 -- gravity
 p.dy+=.03

 -- boost
 if(btn(üÖæÔ∏è)) then
		p.dx+=.1*sin(p.a+.5)
		p.dy+=.1*cos(p.a+.5)
		sfx(0)
	end

 -- shoot
 if(btnp(‚ùé)) then
  sfx(1)
  local s=ship(p.a)[1]
  add(shots, {x=p.x+s.x,y=p.y+s.y,a=p.a})
 end

 -- animate shots 
 for shot in all(shots) do
		shot.x=shot.x+4*sin(shot.a+.5)
		shot.y=shot.y+4*cos(shot.a+.5)
		local f = fget(mget(shot.x/8,shot.y/8),0)
		if(f or shot.x<0 or shot.x>128 or shot.y<0 or shot.y>128) then
   del(shots,shot)
		end
 end

 -- crash in wall
	if(col(p.x+p.dx,p.y,p.a)) then
  crash()
	end

 -- crash in floor/roof
	if(col(p.x,p.y+p.dy,p.a)) then
  local smooth=p.dy > 0 and p.dy < 1 and (p.a>.9 or p.a<.1)
  if (smooth) then
	  p.dy=0
	  p.dx=0
	  p.a=0
	 else
	  crash()
	 end
	end

 -- apply movement
 p.y+=p.dy
 p.x+=p.dx
end
