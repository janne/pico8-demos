__lua__
-- scroll

padding="                                "
text=padding.."hello, world! how are you? this is my scrolling text... "
i=0

function _draw()
 cls()
 local from=flr(i/4)
 local to=from+32
 local t=sub(text,from,to)
 print(t,-(i%4),0)
 i+=1
 if(i>#text*4) i=0
end
